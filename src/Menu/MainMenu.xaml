<Page x:Name="mainMenu" x:Class="Starcraft_BO_helper.MainMenu"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Starcraft_BO_helper"
        mc:Ignorable="d"
        Title="MainWindow" Height="377" Width="594" Background="#FF2B2B2B">
    <StackPanel Margin="0" HorizontalAlignment="Center" VerticalAlignment="Center">
        <Button x:Name="selectButton" Content="Play" HorizontalAlignment="Center" Margin="0" Height="46.324" Width="113.312" Background="#FF5D5D5D" Foreground="White" Click="SelectMenu" FontSize="16" BorderBrush="Black"/>
        <Button x:Name="selectButton_Copy" Content="Create a BO" HorizontalAlignment="Center" Margin="0,30,0,0" Height="27.93" Width="108.312" Background="#FF5D5D5D" Foreground="White" Click="CreateBOClick" FontSize="16" BorderBrush="Black"/>
        <Button x:Name="selectButton_Copy1" Content="Settings" HorizontalAlignment="Center" Margin="0,30,0,0" Height="27.93" Width="108.312" Background="#FF5D5D5D" Foreground="White" Click="SettingsClick" FontSize="16" BorderBrush="Black"/>
    </StackPanel>
</Page>
